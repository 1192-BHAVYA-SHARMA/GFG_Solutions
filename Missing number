//User function Template for Java


class Compute {
    
    public static int missingNumber(int A[], int N)
    {
         // Your code goes here
        int sum = ((N + 1) * (N)) / 2;
        for (int i = 0; i < N; i++)
            sum -= A[i];
        return sum;
    }
}
