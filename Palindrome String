link: https://practice.geeksforgeeks.org/problems/palindrome-string0817/0


//User function Template for Java

class Solution {
    int isPalindrome(String S) {
        // code here
        String s = S;
        int n =S.length();
        for(int i=0; i<n;i++){
            if(s.charAt(i)!=s.charAt(n-i-1)){ return 0;}
        }
        return 1;
    }
};
