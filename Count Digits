link:https://practice.geeksforgeeks.org/problems/count-digits5716/1
question: Count Digits
class Solution{
    static int evenlyDivides(int N){
        // code here
        int t = N;
        int count =0;
        while(t != 0){
            int d = t%10;
            if(( d !=0) && (N % d == 0)){count++;}
            t=t/10;
        }
        return count;
    }
}
