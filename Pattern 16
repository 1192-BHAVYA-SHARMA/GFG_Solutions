link: https://practice.geeksforgeeks.org/problems/triangle-pattern-1662285334/0


class Solution {

    void printTriangle(int n) {
        
        for(int i=0;i<n;i++){
            int ch=65;
            for(int j=0;j<=i;j++){
                System.out.print((char)(ch+i));
            }
            System.out.println("");
        }
    }
}
