// User function Template for Java

class Solution {
    static int dataTypeSize(String str) {
        // code here
        int i=0;
        if(str.equals("Character")){ i=1;}
        else if(str.equals("Integer")){ i=4;}
        else if(str.equals("Long")){ i=8;}
        else if(str.equals("Float")){ i=4;}
        else if(str.equals("Double")){ i=8;}
        return i;
    }
}
