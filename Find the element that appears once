//User function Template for Java

class Sol
{
    public static int search(int A[], int N)
    {
        // your code here
        // XOR all the elements:
        int xorr = 0;
        for (int i = 0; i < N; i++) {
            xorr = xorr ^ A[i];
        }
        return xorr;
    }
}
