link: https://practice.geeksforgeeks.org/problems/triangle-number-1661489840/0

class Solution {

    void printTriangle(int n) {
        // code here
        for(int i=n;i>0;i--){
            for(int j=1;j<=i;j++){
                System.out.print(j+" ");
            }
            System.out.println("");
        }
    }
    }
