link:https://practice.geeksforgeeks.org/problems/triangle-pattern-1661718712/0

class Solution {

    void printTriangle(int n) {
        int i=1;
        for(int j=0;j<n;j++){
            for(int k=0;k<=j;k++){
                System.out.print(i+" ");
                i=i+1;
            }
            System.out.println("");
        }
    }
}
