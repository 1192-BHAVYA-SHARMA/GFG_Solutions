link: https://practice.geeksforgeeks.org/problems/triangle-number-1661428795/0


class Solution {

    void printTriangle(int n) {
        
        for(int i=1;i<=n;i++){
            for(int j=1;j<=i;j++){
                System.out.print(i+" ");
            }
            System.out.println();
        }
    }
}
